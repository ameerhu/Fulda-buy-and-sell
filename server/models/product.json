{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "postedDate": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "sold": {
      "type": "boolean"
    },
    "image": {
      "type": [
        "string"
      ],
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "customerToWish": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "product_Id",
      "through": "Wishlist"
    }
  },
  "acls": [],
  "methods": {}
}
