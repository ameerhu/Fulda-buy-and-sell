{
  "name": "Customer",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "products": {
      "type": "embedsMany",
      "model": "Product",
      "property": "products",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "productsInWish": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "customer_Id",
      "through": "Wishlist"
    },
 
    "senderReceiverRelation": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "customer_Id",
      "through": "Message"
    }
  },
  "acls": [],
  "methods": {}
  
}

